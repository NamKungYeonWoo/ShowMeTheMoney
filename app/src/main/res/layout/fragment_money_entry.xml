<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffffff">

    <RelativeLayout
        android:id="@+id/input1"
        android:layout_width="wrap_content"
        android:layout_height="48.0dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="4.0dip"
            android:layout_marginLeft="14.0dip"
            android:src="@drawable/ic_date_range_black_24dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="1.0px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="14.0dip"
            android:layout_marginRight="14.0dip"
            android:layout_toRightOf="@id/imageView1"
            android:background="#ffdddddd" />

        <TextView
            android:id="@+id/tvPickDate"
            android:layout_width="wrap_content"
            android:layout_height="30.0dip"
            android:layout_above="@id/textView3"
            android:layout_alignLeft="@id/textView3"
            android:layout_alignRight="@id/textView3"
            android:layout_marginBottom="4.0dip"
            android:gravity="center"
            android:textAppearance="?android:textAppearanceMedium"
            android:textColor="#ff474747"
            tools:text="2015. 09. 16(수)" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/input2"
        android:layout_width="wrap_content"
        android:layout_height="50.0dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/input1">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="4.0dip"
            android:layout_marginLeft="14.0dip"
            android:src="@drawable/ic_catecory" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="1.0px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="14.0dip"
            android:layout_marginRight="14.0dip"
            android:layout_toRightOf="@id/imageView11"
            android:background="#ffdddddd" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="30.0dip"
            android:layout_above="@id/textView33"
            android:layout_alignLeft="@id/textView33"
            android:layout_alignRight="@id/textView33"
            android:layout_marginBottom="4.0dip"
            android:gravity="center"
            android:textAppearance="?android:textAppearanceMedium"
            android:textColor="#ff474747"
            tools:text="식비" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/input22"
        android:layout_width="wrap_content"
        android:layout_height="48.0dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/input2">

        <ImageView
            android:id="@+id/imageView1111"
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="4.0dip"
            android:layout_marginLeft="14.0dip"
            android:src="@drawable/ic_description_black_24dp" />

        <TextView
            android:id="@+id/textView3333"
            android:layout_width="wrap_content"
            android:layout_height="1.0px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="14.0dip"
            android:layout_marginRight="14.0dip"
            android:layout_toRightOf="@id/imageView1111"
            android:background="#ffdddddd" />

        <EditText
            android:id="@+id/textView6666"
            android:layout_width="wrap_content"
            android:layout_height="30.0dip"
            android:layout_above="@id/textView3333"
            android:layout_alignLeft="@id/textView3333"
            android:layout_alignRight="@id/textView3333"
            android:layout_marginBottom="4.0dip"
            android:gravity="center"
            android:hint="상세 설명"
            android:textAppearance="?android:textAppearanceSmall"
            android:textColor="#ffaaaaaa" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/input3"
        android:layout_width="wrap_content"
        android:layout_height="48.0dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/input22">

        <ImageView
            android:id="@+id/imageView111"
            android:layout_width="30.0dip"
            android:layout_height="30.0dip"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="4.0dip"
            android:layout_marginLeft="14.0dip"
            android:src="@drawable/ic_currency" />

        <TextView
            android:id="@+id/textView333"
            android:layout_width="wrap_content"
            android:layout_height="1.0px"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="14.0dip"
            android:layout_marginRight="14.0dip"
            android:layout_toRightOf="@id/imageView111"
            android:background="#ffdddddd" />

        <TextView
            android:id="@+id/textView666"
            android:layout_width="wrap_content"
            android:layout_height="30.0dip"
            android:layout_above="@id/textView333"
            android:layout_alignLeft="@id/textView333"
            android:layout_alignRight="@id/textView333"
            android:layout_marginBottom="4.0dip"
            android:gravity="center"
            android:text="0"
            android:textAppearance="?android:textAppearanceMedium"
            android:textColor="#ff474747" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/input3"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="1"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="2"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="3"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/TextView03"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="4"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView02"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="5"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView01"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="6"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/TextView04"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="7"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView05"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="8"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView06"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="9"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0dip"
            android:layout_weight="1.0">

            <TextView
                android:id="@+id/TextView07"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="00"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView09"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="0"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/TextView08"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="C"
                android:textAppearance="?android:textAppearanceLarge"
                android:textColor="#ff474747"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>